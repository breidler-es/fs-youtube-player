{
  "name": "fs-youtube-player",
  "version": "1.0.1",
  "description": "FirstSpirits Shoppable (YouTube) Video Player - Demo",
  "main": "./dist/fs-youtube-player.umd.js",
  "module": "./dist/fs-youtube-player.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "rollup -c",
    "demo": "npm run build -- --watch",
    "format": "npm run clean && prettier --write ."
  },
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "e-Spirit",
    "FirstSpirit",
    "YouTube",
    "Shoppable",
    "Shoppable Video",
    "Interactive Video"
  ],
  "author": "e-Spirit AG",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/e-Spirit/fs-youtube-player.git"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^6.1.0"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
